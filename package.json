        "winston": "^3.0.0",
        "winston-cloudwatch": "^2.0.5"
    },
    "devDependencies": {
        "@nestjs/testing": "^5.4.0",
        "@types/aws-lambda": "^8.10.7",
        "@types/aws-serverless-express": "^3.3.0",
        "@types/express": "^4.16.0",
        "@types/imagemagick": "0.0.30",
