{
    "name": "rankings",
    "version": "0.0.1",
    "description": "ISA Rankings Backend",
    "author": "Can Sahin",
    "license": "MIT",
    "scripts": {
        "format": "prettier --write \"**/*.ts\"",
        "build": "serverless webpack",
        "start": "export ENVIRONMENT=Local; serverless offline start",
        "deploy": "serverless deploy --stage Dev",
        "deploy:prod": "serverless deploy --stage Prod",
        "deploy:api": "serverless deploy -f WebAppAPI",
        "deploy:admin": "serverless deploy -f AdminAPI",
        "deploy:streams": "serverless deploy -f Streams",
        "deploy:image-resizer": "serverless deploy -f Image-Resizer",
        "deploy:cron-job": "serverless deploy -f Cron-Job",
        "test": "export ENVIRONMENT=Local; jest",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "webpack": "webpack --config webpack.config.js"
    },
    "dependencies": {
        "@nest-middlewares/helmet": "^5.1.1",
        "@nest-middlewares/method-override": "^5.1.1",
        "@nest-middlewares/morgan": "^5.1.1",
