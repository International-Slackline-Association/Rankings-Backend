{
  "name": "rankings",
  "version": "0.0.1",
  "description": "ISA Rankings Backend",
  "author": "Can Sahin",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "build": "serverless webpack",
    "start": "export ENVIRONMENT=Local; serverless offline start",
    "test": "export ENVIRONMENT=Local; jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "fastify-formbody": "^2.0.0",
    "lodash": "^4.17.10",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "serverless-http": "^1.6.0",
    "typescript": "^2.6.2",
    "winston": "^3.0.0",
    "winston-cloudwatch": "^1.13.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/aws-lambda": "^8.10.7",
    "@types/express": "^4.0.39",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.5",
    "@types/supertest": "^2.0.4",
    "aws-sdk": "^2.263.1",
    "jest": "^23.1.0",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "serverless-dynamodb-autoscaling": "^0.6.2",
    "serverless-offline": "^3.25.4",
    "serverless-prune-plugin": "^1.3.1",
    "serverless-webpack": "^5.1.5",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.1.0",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.10.0",
    "tslint-loader": "^3.6.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  },
  "peerDependencies": {
    "winston": "^2.1.1"
  }
}
